/**
 * represents a commit reference for bibtex reference
 * @author daniel
 *
 */
public class CommitReference {
	String id;
	String author;
	String date;
	
	public CommitReference(String id, String author, String date) {
		this.id = id;
		this.author = author;
		this.date = date;
	}
	
	public shortId() {
		return this.id.subSequence(0, 7);
	}
}
